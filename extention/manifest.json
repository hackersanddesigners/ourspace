{
  "name": "Ourspace",
  "version": "0.1",
  "description": "The Ourspace bot extension.",
  "permissions": [ "activeTab", "declarativeContent", "storage" ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html"
   },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/"],
      "js": [ "bot.js", "scenes.js" ]
    }
  ],
  "manifest_version": 2
}
